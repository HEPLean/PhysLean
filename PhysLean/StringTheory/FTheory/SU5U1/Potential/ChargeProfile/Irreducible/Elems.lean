/-- OfNat instance on `Option ℤ`. -/
local instance (n : ℕ) : OfNat (Option ℤ) n where
  ofNat := some n

/-- Negation on `Option ℤ`. -/
local instance : Neg (Option ℤ) where
  neg x := match x with
    | none => none
    | some n => some (-n)

